package 산술연산자;

public class Ex04증감연산자 {

	public static void main(String[] args) {

		int num = 4;
		// 증감연산자 --> 위치에 따라서 증가되는 순서가 변동
		// ++변수 > 1증가 >> 변수 사용 (다음줄로 넘어가면, 사용되었다고 봄!)
		// 변수++ > 변수 사용 >> 1증가 
		System.out.println(++num); 
		System.out.println(num);
		System.out.println(num++);
		System.out.println(num);
		
		
		// 비교연산자 : == > 같다 / != > 같지 않다
		// 논리연산자 : !(not), &&(and), ||(or)
		// &&(and) : 하나라도 거짓이면 거짓이 된다.
		// ||(or) : 하나라도 참이면 참이 된다
		//
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
